<?xml version="1.0"?>
<robot name="aubo_e5" xmlns:xacro="http://ros.org/wiki/xacro">

 <!-- Defining the colors used in this robot -->
    <material name="Black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="White">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="Blue">
        <color rgba="0 0 1 1"/>
    </material>

    <material name="Red">
        <color rgba="1 0 0 1"/>
    </material>

    <material name="Grey">
        <color rgba="0.96 0.96 0.96 1"/>
    </material>

    <!-- Constants -->
    <xacro:property name="M_PI" value="3.14159"/>







    <!--/////////////////// DYNAMICS PARAM //////////////////////-->

    <xacro:macro name="E5_Shoulder_Inertia">
        <origin
        xyz="-2.1059E-08 0.0083051 0.065103"
        rpy="0 0 0" />
      <mass
        value="1.5715" />
      <inertia
        ixx="0.0030796"
        ixy="6.3642E-08"
        ixz="1.9408E-08"
        iyy="0.0028016"
        iyz="-0.00021308"
        izz="0.0031979" />
    </xacro:macro>

    <xacro:macro name="E5_UpperArm_Inertia">
       <origin
        xyz="0.204 -0.00063844 0.057604"
        rpy="0 0 0" />
      <mass
        value="4.9786" />
      <inertia
        ixx="0.0075025"
        ixy="3.8877E-05"
        ixz="-1.4653E-08"
        iyy="0.019967"
        iyz="-4.6361E-06"
        izz="0.020135" />
    </xacro:macro>

    <xacro:macro name="E5_ForeArm_Inertia">
        <origin
        xyz="0.18368 0.00064681 0.055339"
        rpy="0 0 0" />
      <mass
        value="2.2775" />
      <inertia
        ixx="0.00182"
        ixy="-2.5051E-05"
        ixz="-8.9044E-05"
        iyy="0.0088861"
        iyz="-1.3488E-06"
        izz="0.0089876" />
    </xacro:macro>

    <xacro:macro name="E5_Wrist1_Inertia">
